uniform vec3 GhostScale;
uniform vec3 GhostTranslate;
uniform float GhostAngle;
uniform float time;
uniform float S;
uniform float freq;

varying vec2 Texcoord;
mat4 rotX (float angleRad)
{
   float PI = 3.1415926535897932384626433832795;
   float angle = (PI / 180) * angleRad;
   float s = sin(angle);
   float c = cos(angle);

   return mat4(
   1, 0, 0, 0,
   0, c,-s, 0,
   0, s, c, 0,
   0, 0, 0, 1); 
}

void main(void)
{ 
   vec4 inPos=gl_Vertex;
   inPos.xyz *=  GhostScale; 
   mat4 rotMatrixX = rotX( GhostAngle);
   inPos = rotMatrixX * inPos;
   inPos.xyz += ( GhostTranslate);
   float disp = S * sin(freq * time + inPos.x *inPos.y);
   inPos.xyz += disp * normalize(gl_Normal);
   gl_Position = gl_ModelViewProjectionMatrix * inPos; 
   Texcoord = gl_MultiTexCoord0.xy;  
}]]></RmShaderCode></RmGLShader>
