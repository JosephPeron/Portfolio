    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D baseMap;

varying vec2 Texcoord;
varying float fadingRate;

void main( void )
{
    //vec4 baseColor = texture2D( baseMap, Texcoord );
     float radius = 5;

   float x = Texcoord.x;
   float y = Texcoord.y;

   float range = radius * radius - dot(Texcoord - 0.5, Texcoord - 0.5);

   float shade = 2 / (1 + pow(2.71828, 12 * range));

   vec4 baseColour = (1 - shade) * texture2D(baseMap, Texcoord);

   gl_FragColor = baseColour * fadingRate;
    
}]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform mat4  viewInv;
uniform float time;
uniform float freq;
uniform float lifecycle;
uniform float amp;
uniform vec3 translate;

varying vec2 Texcoord;
varying float fadingRate;

void main( void )
{
    vec4 bbPos = gl_Vertex;
    //Texcoord    = gl_MultiTexCoord0.xy;
    Texcoord = sign(bbPos.xy)*0.5 + 0.5;
    vec2 billboardSize = (4,4);
    bbPos.xy *= billboardSize;
    vec3 Vx=viewInv[0].xyz;   
    vec3 Vy=viewInv[1].xyz; 
    bbPos.xyz = (bbPos.x * Vx+ bbPos.y * Vy).xyz;
    bbPos.xyz += (translate);
    float T=amp * mod(freq *(time + (gl_Vertex.z * 10)) , lifecycle);
    //bbPos.xyz +=v*T;
    float particleSpread = 2;
    float particleSystemHeight = 5;
    vec3 v;
    v.x = particleSpread * T * cos(300*gl_Vertex.z);
    v.z = particleSpread * T * sin(300*gl_Vertex.z);
    v.y = particleSystemHeight * T;
    //bbPos.xyz +=vec3(20.0);
    bbPos.xyz += v;
    fadingRate = (1.0 -T/lifecycle);
    gl_Position = gl_ModelViewProjectionMatrix*bbPos;
}]]></RmShaderCode></RmGLShader>
