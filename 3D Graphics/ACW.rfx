uniform float snakeAngle;
uniform vec3 snakeScale;
uniform vec3 snakeTranslate;
uniform float time;

varying vec2 Texcoord;

mat4 rotX (float angleRad)
{
   float PI = 3.1415926535897932384626433832795;
   float angle = (PI / 180) * angleRad;
   float s = sin(angle);
   float c = cos(angle);

   return mat4(
   1, 0, 0, 0,
   0, c,-s, 0,
   0, s, c, 0,
   0, 0, 0, 1); 
}

void main(void)
{ 
   Texcoord = gl_MultiTexCoord0.xy;

   vec4 inPos=gl_Vertex;
   
   if(inPos.y > -10 && inPos.y < 20) {
      inPos.x += sin(time * 0.3 * inPos.y);
   }
   inPos.xyz *= snakeScale; 
   mat4 rotMatrixX = rotX(snakeAngle);
   inPos = rotMatrixX * inPos;
   inPos.xyz += snakeTranslate;
   
   gl_Position = gl_ModelViewProjectionMatrix * inPos;
}]]></RmShaderCode></RmGLShader>
