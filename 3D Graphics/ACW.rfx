    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float TableAngle;
uniform vec3 TableScale;

varying vec2 Texcoord;
varying vec3 Normal;
varying vec4 ObjectPosition;

mat4 rotX (float angleRad)
{
   float PI = 3.1415926535897932384626433832795;
   float angle = (PI / 180) * angleRad;
   float s = sin(angle);
   float c = cos(angle);

   return mat4(
   1, 0, 0, -0,
   0, c,-s, 0,
   0, s, c, 0,
   0, 0, 0, 1); 
}

mat3 rotN (float angleRad)
{
   float PI = 3.1415926535897932384626433832795;
   float angle = (PI / 180) * angleRad;
   float s = sin(angle);
   float c = cos(angle);

   return mat3(
   1, 0, 0,
   0, c,-s,
   0, s, c); 
}

void main(void)
{
   Texcoord = gl_MultiTexCoord0.xy;
 
   vec4 inPos=gl_Vertex;
  
   ObjectPosition = gl_ModelViewMatrix * inPos;
   
   Normal = gl_NormalMatrix * gl_Normal;
   mat3 rotMatrixN = rotN(TableAngle);
   Normal = rotMatrixN * Normal;
   Normal = normalize(Normal);

   inPos.xyz *= TableScale;
   mat4 rotMatrixX = rotX(TableAngle);
   inPos = rotMatrixX * inPos;
   
   gl_Position = gl_ModelViewProjectionMatrix * inPos;
}
   ]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D tableBase;
uniform vec4 fvAmbient;
uniform vec4 fvDiffuse;
uniform vec3 fvLightPosition;

varying vec2 Texcoord;
varying vec4 ObjectPosition;
varying vec3 Normal;

void main(void)
{       
   vec4  baseColour = texture2D( tableBase, Texcoord);
   vec3 LightDirection = normalize(fvLightPosition - ObjectPosition.xyz);
   vec4 IDiff = fvDiffuse * max(dot(Normal,LightDirection), 0.0);
   IDiff = clamp(IDiff, 0.0, 1.0); 

   vec4 fvTotalAmbient  = fvAmbient * baseColour;
   vec4 totalIDiff      = IDiff * baseColour;

   gl_FragColor = fvTotalAmbient + totalIDiff;
   
   


       
}]]></RmShaderCode></RmGLShader>
